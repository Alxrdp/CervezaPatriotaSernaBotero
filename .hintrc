{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-behavior",
          "text-decoration-skip-ink",
          "text-align: -webkit-match-parent",
          "-webkit-overflow-scrolling",
          "overflow-anchor"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 110",
    "not chrome <= 110",
    "not edge <= 110",
    "not firefox <= 110",
    "not safari <= 16.3",
    "not samsung <= 20",
    "not and_ff <= 110"
  ]
}